{
    "name": "laravel/laravel",
    "description": "The Laravel Framework.",
    "keywords": [
        "framework",
        "laravel"
    ],
    "license": "MIT",
    "type": "project",
    "require": {
        "php": ">=5.6.4",
        "laravel/framework": "^6.4",
        "laravel/socialite": "^4.2",
        "barryvdh/laravel-debugbar": "*",
        "owen-it/laravel-auditing": "*",
        "orangehill/iseed": "*",
        "backup-manager/laravel": "^1.3",
        "thomaswelton/laravel-gravatar": "*",
        "cornford/googlitics": "*",
        "laravelista/ekko": "*",
        "doctrine/dbal": "*",
        "masih/youtubedownloader": "*",
        "srmklive/flysystem-dropbox-v2": "1.0.6",
        "backup-manager/backup-manager": "^1.3",
        "league/flysystem-aws-s3-v3": "*",
        "antennaio/laravel-clyde": "*",
        "illuminate/html": "*",
        "laravelcollective/html": "6.0.*",
        "intervention/image": "*",
        "adrianmejias/laravel-states": "*",
        "predis/predis": "^1.1",
        "fideloper/proxy": "^4.0",
        "laravel/tinker": "^1.0"
    },
    "require-dev": {
        "fzaninotto/faker": "^1.4",
        "mockery/mockery": "^1.0",
        "phpunit/phpunit": "^8.0",
        "symfony/css-selector": "3.1.*",
        "nunomaduro/collision": "^3.0",
        "facade/ignition": "^1.4"
    },
    "autoload": {
        "classmap": [
            "database"
        ],
        "psr-4": {
            "ShowDb\\": "app/"
        }
    },
    "autoload-dev": {
        "classmap": [
            "tests/TestCase.php"
        ]
    },
    "scripts": {
        "post-root-package-install": [
            "php -r \"file_exists('.env') || copy('.env.example', '.env');\""
        ],
        "post-create-project-cmd": [
            "php artisan key:generate"
        ],
        "post-install-cmd": [
            "Illuminate\\Foundation\\ComposerScripts::postInstall"
        ],
        "post-update-cmd": [
            "Illuminate\\Foundation\\ComposerScripts::postUpdate"
        ],
        "post-autoload-dump": [
            "Illuminate\\Foundation\\ComposerScripts::postAutoloadDump",
            "php artisan package:discover"
        ]
    },
    "config": {
        "preferred-install": "dist"
    }
}